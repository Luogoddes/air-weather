n\n            this.region.JWD = selectedRegion.lon + ',' + selectedRegion.lat\n\n            // 保存到cookie\n            this.$cookie.set(\"region\", JSON.stringify(this.region))\n\n            // 保存到vuex\n            this.$store.commit('setLocationData', this.region)\n            window.location.reload()\n            this.$message.success('保存成功')\n          } else {\n            console.error('Region not found')\n          }\n          //this.storeData()\n        })\n        .catch(error => {\n          console.error('Error fetching geo info:', error)\n        })\n    },\n    storeData() {\n      // 在这里实现保存数据的逻辑\n      console.log('数据已保存')\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\">\n.el-input__inner {\n  background-color: transparent !important;\n  color: aliceblue;\n}\n\n@import '../assets/scss/index.scss';\n\n.el-dropdown-link {\n  cursor: pointer;\n  color: #409EFF;\n  font-size: 19px;\n}\n\n.el-icon-arrow-down {\n  font-size: 12px;\n}\n</style>\n"],"mappings":";;;;;;;AAoHA,OAAAA,KAAA;AACA,SAAAC,UAAA;AACA,SAAAC,UAAA;AACA;AACA,OAAAC,SAAA;AACA,SAAAC,UAAA;AACA,OAAAC,WAAA;AACA,OAAAC,WAAA;AACA,OAAAC,YAAA;AACA,OAAAC,YAAA;AACA,OAAAC,MAAA;AACA,OAAAC,UAAA;AACA,OAAAC,WAAA;AACA,OAAAC,OAAA;AACA;EACAC,MAAA,GAAAV,SAAA;EACAW,IAAA,WAAAA,KAAA;IACA;MACAC,MAAA;MACAC,OAAA;MACAC,OAAA;MACAC,QAAA;MACAC,QAAA;MACAC,OAAA;MACAC,eAAA;MACAnB,UAAA,EAAAA,UAAA;MACAoB,eAAA;MAAA;MACAC,mBAAA;MACAC,MAAA;QACAC,IAAA;QACAC,EAAA;QACAC,EAAA,OAAAC,MAAA,CAAAC,OAAA,CAAAC,WAAA;QACAC,GAAA;QACAC,GAAA;QACAC,GAAA;MACA;IACA;EACA;EACAC,UAAA;IACA7B,WAAA,EAAAA,WAAA;IACAC,WAAA,EAAAA,WAAA;IACAC,YAAA,EAAAA,YAAA;IACAC,YAAA,EAAAA,YAAA;IACAC,MAAA,EAAAA,MAAA;IACAC,UAAA,EAAAA,UAAA;IACAC,WAAA,EAAAA;EACA;EACAwB,OAAA,WAAAA,QAAA;IACA,KAAAC,MAAA;IACA,KAAAC,aAAA;EACA;EACAC,aAAA,WAAAA,cAAA;IACAC,aAAA,MAAAxB,MAAA;EACA;EACAyB,OAAA,EAAAC,aAAA,CAAAA,aAAA,KACAxC,UAAA;IACAyC,aAAA,WAAAA,cAAAC,OAAA;MACA,KAAAC,QAAA,CAAAC,OAAA,aAAAF,OAAA;MACA/B,OAAA,CAAAkC,GAAA,UAAAH,OAAA;QAAAI,OAAA;MAAA;MACA,KAAAC,QAAA,CAAAL,OAAA;IAEA;IACAP,MAAA,W